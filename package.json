{
  "name": "pratt-parser",
  "version": "0.0.0-semantic-release",
  "main": "dist/parser.js",
  "jsnext:main": "src/parser.js",
  "module": "src/parser.js",
  "description": "TDOP parser",
  "keywords": [
    "tokenizer",
    "parser",
    "PRATT",
    "TDOP"
  ],
  "engines": {
    "node": ">=6.9.1"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/pratt-parser/issues"
  },
  "homepage": "https://github.com/arlac77/pratt-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "2.11.15",
    "istanbul": "^0.4.5",
    "markdown-doctest": "^0.9.0",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "rollup": "^0.36.3",
    "semantic-release": "^6.3.2",
    "cz-conventional-changelog": "^1.2.0",
    "jsdoc-to-markdown": "^2.0.1"
  },
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/parser.js -- src/parser.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/parser.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/pratt-parser.git"
  },
  "contributors": [{
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
