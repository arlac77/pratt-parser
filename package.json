{
  "name": "pratt-parser",
  "version": "0.0.0-semantic-release",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "description": "TDOP parser",
  "keywords": [
    "tokenizer",
    "parser",
    "PRATT",
    "TDOP"
  ],
  "engines": {
    "node": ">=6.9.1"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/pratt-parser/issues"
  },
  "homepage": "https://github.com/arlac77/pratt-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "2.11.15",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "semantic-release": "^6.3.3",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "build": "node_modules/rollup/bin/rollup --output=dist/parser.js -c -- src/parser.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/parser.js > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/pratt-parser.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}