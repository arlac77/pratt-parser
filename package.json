{
  "name": "pratt-parser",
  "version": "0.0.0-semantic-release",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "description": "TDOP parser",
  "keywords": [
    "tokenizer",
    "parser",
    "PRATT",
    "TDOP"
  ],
  "engines": {
    "node": ">=7.10.0"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/pratt-parser/issues"
  },
  "homepage": "https://github.com/arlac77/pratt-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.0.2",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.2",
    "rollup": "^0.42.0",
    "semantic-release": "^6.3.6"
  },
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/parser.js -c -- src/parser.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/pratt-parser.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}