{
  "name": "pratt-parser",
  "version": "0.0.0-semantic-release",
  "main": "dist/parser.js",
  "jsnext:main": "src/parser.js",
  "module": "src/parser.js",
  "description": "TDOP parser",
  "keywords": [
    "tokenizer",
    "parser",
    "PRATT",
    "TDOP"
  ],
  "engines": {
    "node": ">=6.3.0"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/pratt-parser/issues"
  },
  "homepage": "https://github.com/arlac77/pratt-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "3.4.2",
    "markdown-doctest": "^0.8.1",
    "mocha": "3.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "rollup": "^0.36.3",
    "semantic-release": "^6.3.1"
  },
  "scripts": {
    "cover": "npm run build && node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "npm run build &&  ./node_modules/.bin/jsdoc -r src/*.js",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/parser.js -- src/parser.js",
    "test": "npm run build && mocha --harmony_shipping tests/*.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/pratt-parser.git"
  }
}
